        <div class="tab-content mt-30" id="myTabContent">
                    <!-- -----------------first tab----------------- -->
            <div class="tab-pane fade show active" id="profile" role="tabpanel" aria-labelledby="profile-tab">
                <!-- loop to store threads from database that are located in the threads object array -->
                <% for(let i=0; i < threads.length; i++) { if(threads[i].range === "Global"){%>
                    <div class="single-forum-post-widget wow fadeInUp">
                        <div class="post-content">
                            <div class="post-title">
                                <span style="display: none;"><%= threads[i]._id %></span> 
                                <h6><a href="/messages/<%= threads[i]._id %>" class="topic"><span><%= threads[i].topic %></span></a>
                                    <span><i class="icon_check_alt2"></i></span>
                                </h6>
                            </div>
                            <div class="post-info">
                                <div class="author">
                                    <% if(threads[i].bIsAnonPost === true){ %>
                                        <img src="img/user-circle-alt.svg" alt="icon"><span>Anonymous</span>
                                    <% }else{ %>
                                     <img src="img/user-circle-alt.svg" alt="icon"><span><%= threads[i].postedBy %></span>
                                    <% } %>
                                </div>

                                <div class="post-time">
                                    <img src="img/time-outline.svg" alt=""><%= threads[i].date.toLocaleString() %>
                                </div>
                                <div class="post-category">
                                    <img src="img/home_three/forum-catagory-02.svg" alt="">Visibility: <%= threads[i].range%>
                                </div>
                            </div>
                            <div class="post-tags">
                                <% for (let j=0; j < threads[i].tags.length; j++) { %>
                                    <div class="single-tag"><%= threads[i].tags[j] %></div>
                                <% } %> 
                            </div>
                        </div>
                        <div class="post-reach">
                            <div class="post-view makeVisible">
                                <img src="img/forum/eye-outline.svg" alt="icon"><%= threads[i].views %> Views
                            </div>
                            <div class="post-comment">
                                <img src="img/forum/chatbubbles-outline.svg" alt="icon"><%= threads[i].messages.length %> Replies
                            </div>
                            <div class="post-like">
                                <button class = "btn-primary postbutton"><img src="img/forum/thumbs-up-outline.svg" alt="icon"><%= threads[i].likes %> Likes</button>
                            </div>
                            <div class="post-dislike">
                                <button class = "btn-primary postbutton"><i class="fa-solid fa-thumbs-down"></i> <%= threads[i].dislikes %> DisLikes</button>
                            </div>
                            <% if (threads[i].postedBy === user.username){ %>
                                <div class="post-delete">
                                    <button class = "btn-primary postbutton"><i class="fa-solid fa-circle-minus"></i>Delete</button>
