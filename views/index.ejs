                    <!-- <h4 class="widget-title">Create New Thread</h4> -->
                    <form id="newThreadForm">
                        <input type="hidden" value="<%= String(user.username) %>" name="postedBy">
                      <div class="mt-30">
                        <label class="label" for="inp_title">Topic<span>*</span></label>
                        <div class="icon-input-group">
                          <input class="form-control" type="text" id="inp_title" name="topic" required maxlength="120"/>
                          <i class="icon_chat_alt"></i>
                        </div>
                        <div class="instruction">
                          Please choose an appropriate topic for the thread
                        </div>
                      </div>
                      <div class="mt-25">
                        <label class="label" for="range">Viewable Range (Miles)<span>*</span></label>
                        <div class="icon-input-group">
                          <select id="range" class="custom-select form-control" name="range">
                            <option value="2">2</option>
                            <option value="Global">Global</option>
                            <option value=".5">.5</option>
                            <option value="1">1</option>
                            <option value="5">5</option>
                            <option value="10">10</option>
                            <option value="20">20</option>
                          </select>
                          <i class="icon_folder-open"></i>
                        </div>
                        <div class="instruction">
                            This is the radius in miles that your post will be viewable from.
                        </div>
                      </div>
                      <div class="mt-25">
                        <label class="label" for="inp_tag">Tags</label>
                        <div class="icon-input-group">
                          <input class="form-control" type="text" id="inp_tag" name="tags" />
                          <i class="icon_tags"></i>
                        </div>
                        <div class="instruction">
                          Please comma split tags and choose suitable Keywords Ex:
                          <span>question, event, rant, compliment.</span>
                        </div>
                      </div>
                      <!-- <div class="mt-25">
                        <label class="label" for="inp_tag">Image</label>
